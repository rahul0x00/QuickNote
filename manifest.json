{
  "manifest_version": 2,
  "name": "QuickNote",
  "version": "1.0",
  "description": "A simple extension for taking quick notes.",
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Quick Note",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}

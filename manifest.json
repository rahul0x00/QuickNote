{
  "manifest_version": 2,
  "name": "QuickNote",
  "version": "1.1",
  "description": "A simple note-taking extension",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { 
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
